        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">beta-thalassemia Genotype</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0007685 -->

    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0007685">
        <rdfs:subClassOf rdf:resource="http://scdontology.h3abionet.org/ontology/SCDO_0001336"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;A decrease in the amount of oxygen in the body. Symptoms range from mild (impaired judgment, memory loss, impaired motor coordination) to severe (seizures and coma). [Curator team: []]&apos;</obo:IAO_0000115>
        <ontology:SCDO_0000340 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://purl.obolibrary.org/obo/NCIT_C3890</ontology:SCDO_0000340>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NCIT:C3890</oboInOwl:hasDbXref>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hypoxia</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0007716 -->

    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0007716">
