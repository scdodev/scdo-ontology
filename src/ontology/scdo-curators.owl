        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;In situ death of bone within the humeral head due to disruption of blood supply [Curator team: SCD Management guidelines - Jamaica []]&apos;</obo:IAO_0000115>
        <ontology:SCDO_0000303 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Has synonym: Osteonecrosis of the humeral head</ontology:SCDO_0000303>
        <ontology:SCDO_0000340 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.wheelessonline.com/ortho/Humeral_Head_Osteonecrosis</ontology:SCDO_0000340>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Avascular necrosis of the humeral head</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0007624 -->

    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0007624">
