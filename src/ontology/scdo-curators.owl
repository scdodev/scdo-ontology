        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wechsler Intelligence Scale for Children-Fifth Edition (WISC-V)</oboInOwl:hasExactSynonym>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Intelligence Scale - 6 to 16 years 11 months</rdfs:label>
    </owl:Class>
    


