        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;An abnormally low number of neutrophils in the peripheral blood.&apos;</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">neutropenia</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0003089 -->

    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0003089">
        <rdfs:subClassOf rdf:resource="http://scdontology.h3abionet.org/ontology/scdo.owl/SCDO_1000001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;A state of disinterestedness, listlessness, and indifference, resulting in difficulty performing simple tasks or concentrating.&apos;</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">lethargy</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0003118 -->

    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0003118">
        <rdfs:subClassOf rdf:resource="http://scdontology.h3abionet.org/ontology/scdo.owl/SCDO_1000056"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;An increased amount of bilirubin in the blood.&apos;</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hyperbilirubinemia</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0003119 -->

    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0003119">
        <rdfs:subClassOf rdf:resource="http://scdontology.h3abionet.org/ontology/SCDO_0000922"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;Erythromycin is a macrolide antibiotic with a 14-carbon ring that has an antimicrobial spectrum similar to or slightly wider than that of penicillin, and is often used for people that have an allergy to penicillins. Erythromycin may possess bacteriocidal activity, particularly at higher concentrations by binding to the 50S subunit of the bacterial 70S rRNA complex, inhibiting peptidyl-tRNA translocation. Thus, protein synthesis and subsequently structure/function processes critical for life or replication are inhibited. [Curator team: SCD Management guidelines - South Africa []]&apos;</obo:IAO_0000115>
        <ontology:SCDO_0000340 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://purl.obolibrary.org/obo/ARO_0000006</ontology:SCDO_0000340>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ARO:0000006</oboInOwl:hasDbXref>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Erythromycin</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0003158 -->

    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0003158">
        <rdfs:subClassOf rdf:resource="http://scdontology.h3abionet.org/ontology/SCDO_0009818"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;Infrequent menses (less than 6 per year or more than 35 days between cycles).&apos;</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">oligomenorrhea</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0003263 -->

    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0003263">
