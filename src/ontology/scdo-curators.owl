        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;Infrequent or difficult evacuation of feces. [Curator team: https://cks.nice.org.uk/sickle-cell-disease#!backgroundsub:4 []]&apos;</obo:IAO_0000115>
        <ontology:SCDO_0000340 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://purl.obolibrary.org/obo/HP_0002019</ontology:SCDO_0000340>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HP:0002019</oboInOwl:hasDbXref>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Constipation</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0003675 -->

    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0003675">
