        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An assessment of the respondent&apos;s educational performance and learning accommodations. [Curator team: []]</obo:IAO_0000115>
        <ontology:SCDO_0000303 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Include under QoL?</ontology:SCDO_0000303>
        <ontology:SCDO_0000340 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.phenxtoolkit.org/index.php?pageLink=browse.protocols&amp;id=840200</ontology:SCDO_0000340>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PX840200</oboInOwl:hasDbXref>
