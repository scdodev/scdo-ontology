        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;Persistance of the imflamtory event in the articulation overtime.&apos;</obo:IAO_0000115>
        <ontology:SCDO_0000303 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Not found in any other ontology, is the term arthritis (SCDO:0000075) sufficient?</ontology:SCDO_0000303>
        <ontology:SCDO_0000340 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ncbi.nlm.nih.gov/mesh/68001168</ontology:SCDO_0000340>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MeSH:D001168</oboInOwl:hasDbXref>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Chronic arthritis</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0008623 -->

    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0008623">
        <rdfs:subClassOf rdf:resource="http://scdontology.h3abionet.org/ontology/SCDO_0000851"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;The obstruction of blood vessels by altered erythrocytes that can result in pain, anemia and tissue ischemia&apos;</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">vaso-occlusion</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0008625 -->

    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0008625">
