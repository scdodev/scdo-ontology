        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Splenectomy</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0003765 -->

    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0003765">
