        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;The presence of chronic increased pressure in the systemic arterial system.&apos;</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hypertension</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0008888 -->

    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0008888">
        <rdfs:subClassOf rdf:resource="http://scdontology.h3abionet.org/ontology/SCDO_0001133"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;Iron chelation therapy is the removal of excess iron from the body with special drugs.&apos;</obo:IAO_0000115>
