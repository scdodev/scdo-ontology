        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">DB00333</oboInOwl:hasDbXref>
        <oboInOwl:hasExactSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">6-(dimethylamino)-4,4-diphenylheptan-3-one</oboInOwl:hasExactSynonym>
