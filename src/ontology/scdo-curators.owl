        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ascending Cholangitis</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0008778 -->

