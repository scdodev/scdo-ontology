        <ontology:SCDO_0000482>Clinic Patients</ontology:SCDO_0000482>
        <ontology:SCDO_0000484>Numerous</ontology:SCDO_0000484>
        <ontology:SCDO_0001033>Widely used around the world, also used in SCD</ontology:SCDO_0001033>
        <ontology:SCDO_0001222>Adults 18 years and older</ontology:SCDO_0001222>
        <ontology:SCDO_0001335>no</ontology:SCDO_0001335>
        <oboInOwl:hasExactSynonym>GAD-7</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym>Generalised Anxiety Disorder Questionnaire</oboInOwl:hasExactSynonym>
        <rdfs:label>Generalised Anxiety Disorder Questionnaire (GAD-7)</rdfs:label>
    </owl:Class>
    


