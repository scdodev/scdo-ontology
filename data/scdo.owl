    <owl:Class rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0000656">
        <rdfs:subClassOf rdf:resource="http://scdontology.h3abionet.org/ontology/SCDO_0000893"/>
        <rdfs:subClassOf>
            <owl:Restriction>
