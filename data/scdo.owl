        <ontology:SCDO_0000303>This term replaced &quot;Avascular Necrosis&quot; in QoL.</ontology:SCDO_0000303>
        <ontology:SCDO_0000340 rdf:resource="http://purl.bioontology.org/ontology/MEDLINEPLUS/C0029445"/>
        <ontology:SCDO_0000363>yes</ontology:SCDO_0000363>
        <ontology:SCDO_0000662>Local pain. Typical images on X-ray.</ontology:SCDO_0000662>
        <oboInOwl:hasExactSynonym>AVN</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym>Aseptic Bone Necrosis</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym>Avascular Bone Necrosis</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym>Avascular Necrosis</oboInOwl:hasExactSynonym>
