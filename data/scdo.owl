        <ontology:SCDO_0000482>All</ontology:SCDO_0000482>
        <ontology:SCDO_0001033>WHO Global Burden of Disease assessment, also used in SCD studies</ontology:SCDO_0001033>
        <ontology:SCDO_0001222>Children and adults</ontology:SCDO_0001222>
        <ontology:SCDO_0001335>no</ontology:SCDO_0001335>
        <oboInOwl:hasExactSynonym>DALY</oboInOwl:hasExactSynonym>
        <rdfs:label>Disability-Adjusted Life Year (DALY)</rdfs:label>
    </owl:Class>
    


