        <ontology:SCDO_0000340 rdf:resource="http://bioportal.bioontology.org/ontologies/NCIT/?p=classes&amp;conceptid=http%3A%2F%2Fncicb.nci.nih.gov%2Fxml%2Fowl%2FEVS%2FThesaurus.owl%23C95504"/>
        <ontology:SCDO_0000363>no</ontology:SCDO_0000363>
