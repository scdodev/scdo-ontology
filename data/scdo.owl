        <dc:description>Abnormal accumulation of fluid in the brain.</dc:description>
        <ontology:SCDO_0000303>Provide source for additional info.</ontology:SCDO_0000303>
        <ontology:SCDO_0000335>HP:0002181</ontology:SCDO_0000335>
