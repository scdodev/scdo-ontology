        <dc:description>Clinical assessment to determine the volume of gas within an individual&apos;s lung.</dc:description>
        <dc:source rdf:resource="http://www.phenxtoolkit.org/index.php?pageLink=browse.protocoldetails&amp;id=810401"/>
