        <oboInOwl:hasExactSynonym>Patient Health Questionnaire-9</oboInOwl:hasExactSynonym>
        <rdfs:label>Patient Health Questionnaire-9 (PHQ-9)</rdfs:label>
    </owl:Class>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0000871 -->

