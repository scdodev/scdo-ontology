        <rdfs:label>has output measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://scdontology.h3abionet.org/ontology/SCDO_0000475 -->

    <owl:ObjectProperty rdf:about="http://scdontology.h3abionet.org/ontology/SCDO_0000475">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <dc:creator>SCDO (Jade Hotchkiss)</dc:creator>
